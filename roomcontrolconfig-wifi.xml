<Extensions>
  <Version>1.5</Version>
  <Panel>
    <PanelId>CoffeService</PanelId>
    <Type>Statusbar</Type>
    <Icon>Concierge</Icon>
    <Order>1</Order>
    <Color>#07C1E4</Color>
    <Name>Заказ напитков</Name>
    <Page>
      <Name>Заказ напитков</Name>
      <Row>
        <Name>Кол-во чашек кофе</Name>
        <Widget>
          <WidgetId>CoffeeCount</WidgetId>
          <Type>Spinner</Type>
          <Options>size=4;style=plusminus</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Кол-во чашек чая</Name>
        <Widget>
          <WidgetId>TeaCount</WidgetId>
          <Type>Spinner</Type>
          <Options>size=4;style=plusminus</Options>
        </Widget>
      </Row>
      <Row>
        <Name>Отправить секретарю</Name>
        <Widget>
          <WidgetId>SendButton</WidgetId>
          <Name>Отправить </Name>
          <Type>Button</Type>
          <Options>size=4</Options>
        </Widget>
      </Row>
      <Options/>
    </Page>
  </Panel>
  <Panel>
    <PanelId>WiFiService</PanelId>
    <Type>Statusbar</Type>
    <Icon>Proximity</Icon>
    <Order>2</Order>
    <Color>#D541D8</Color>
    <Name>WiFi</Name>
    <Page>
      <Name>WiFi</Name>
      <Row>
        <Name>Новогодний режим</Name>
        <Widget>
          <WidgetId>APLedOn</WidgetId>
          <Name>Включить</Name>
          <Type>Button</Type>
          <Options>size=2</Options>
        </Widget>
        <Widget>
          <WidgetId>APLedOff</WidgetId>
          <Name>Отключить</Name>
          <Type>Button</Type>
          <Options>size=2</Options>
        </Widget>
      </Row>
      <Options/>
    </Page>
  </Panel>
</Extensions>
